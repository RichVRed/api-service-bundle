{
  "name": "eleven/api-service-bundle",
  "description": "Integrate API Service into Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Guillem CANAL",
      "email": "gcanal@eleven-labs.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "eleven-labs/api-service": "^0.1",
    "symfony/framework-bundle": "^2.7 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5 || ^5.4",
    "symfony/symfony": "^2.7 || ^3.0",
    "php-http/guzzle6-adapter": "^1.1.1",
    "php-http/httplug-bundle": "^1.0",
    "polishsymfonycommunity/symfony-mocker-container": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^1.0",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "ElevenLabs\\ApiServiceBundle\\": ""
    }
  },
  "suggest": {
    "php-http/httplug-bundle": "Integrate HTTP Client into Symfony"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

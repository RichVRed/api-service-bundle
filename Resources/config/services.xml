<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="api_service.uri_template" class="ElevenLabs\Api\Service\UriTemplate\Rize\RizeUriTemplateAdapter" public="false" />
        <service id="api_service.cache_factory" class="ElevenLabs\ApiServiceBundle\Factory\ConfigCacheFactory" public="false">
            <argument />
            <argument />
        </service>

        <service id="api_service.factory.service" class="ElevenLabs\ApiServiceBundle\Factory\ServiceFactory">
            <argument type="service" id="api_service.uri_factory" />
            <argument type="service" id="api_service.uri_template" />
            <argument type="service" id="api_service.client" />
            <argument type="service" id="api_service.message_factory" />
            <argument type="service" id="api_service.cache_factory" />
        </service>
    </services>
</container>
